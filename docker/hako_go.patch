 func (cm *CallbackManager) handleCallFunction(ctx ContextPtr, funcID, thisArg, argc, argv int32) ValuePtr {
+	cm.mu.RLock()
+	realm := cm.contexts[ctx]
+	handler := cm.CallFunctionHandler
+	cm.mu.RUnlock()
+
+	if realm == nil {
+		return 0
+	}
+	if handler != nil {
+		return handler(realm, funcID, thisArg, argc, argv)
+	}
 	return 0
 }
 
